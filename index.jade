include helpers/variables.jade
include helpers/mixins.jade
doctype html1
html(lang='ru')
  head
    meta(charset='utf-8')
    title Анкета Сухопаровой Анны
    link(href='https://fonts.googleapis.com/css?family=PT+Sans:700,400&subset=latin,cyrillic' rel='stylesheet' type='text/css')
    link(href='css/style.css' rel='stylesheet')
body
  .container-1
    .container-2
      header.header-logos.clearfix
        img.img-logo(src='img/img-logo.png' width=154 height=52 alt='CSSSR')
        img.img-stamp(src='img/img-stamp.png' width=252 height=106 alt='СОВЕРШЕННО СЕКРЕТНО')
      .container-inner
        main
          .main-title
            img.img-act(src='img/img-act.png' width=221 height=118 alt='ДЕЛО')
          section.description
            + personal-data(datas)
          section.skills
            + personal-skills(skills)
          section.js
        footer.footer-date
          form.form-third(action='#')
            label(for='id-date') Дата заполенения
            input#id-date(type='text' name='date' value='27.04.2016')
